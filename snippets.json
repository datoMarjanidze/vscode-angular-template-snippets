{
  "NgClass": {
    "prefix": "ngClass",
    "body": "[ngClass]=\"${0:expression}\""
  },
  "NgClassSingle": {
    "prefix": "[class.name]",
    "body": "[class.${1:name}]=\"${0:expression}\""
  },
