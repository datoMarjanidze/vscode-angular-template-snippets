{
  "RouterLink": {
    "prefix": "routerLink",
    "body": "routerLink=\"${1:route}\"$0"
  },
  "RouterLinkDynamicExpression": {
    "prefix": "[routerLink]",
    "body": "[routerLink]=\"${1:route}\"$0"
  },
  "QueryParams": {
    "prefix": ["queryParams", "[queryParams]"],
    "body": "[queryParams]=\"{${1:name}: ${2:expression}}\"$0"
  },
  "Fragment": {
    "prefix": "fragment",
    "body": "fragment=\"${1:name}\"$0"
  },
  "QueryParamsHandling": {
    "prefix": "queryParamsHandling",
    "body": "queryParamsHandling=\"${1:strategy}\"$0"
  },
  "PreserveFragment": {
    "prefix": "[preserveFragment]",
    "body": "[preserveFragment]=\"${1:expression}\"$0"
  },
  "SkipLocationChange": {
    "prefix": "[skipLocationChange]",
    "body": "[skipLocationChange]=\"${1:expression}\"$0"
  },
  "ReplaceUrl": {
    "prefix": "[replaceUrl]",
    "body": "[replaceUrl]=\"${1:expression}\"$0"
  },
  "State": {
    "prefix": "[state]",
    "body": "[state]=\"{${1:name}: ${2:expression}}\"$0"
  },
  "RelativeTo": {
    "prefix": "[relativeTo]",
    "body": "[relativeTo]=\"${1:expression}\"$0"
  },
  "RouterLinkActive": {
    "prefix": "routerLinkActive",
    "body": "routerLinkActive=\"${1:class}\"$0"
  },
  "RouterLinkActiveOptions": {
    "prefix": "[routerLinkActiveOptions]",
    "body": "[routerLinkActiveOptions]=\"{exact: ${1:expression}}\"$0"
  },
  "RouterOutlet": {
    "prefix": "router-outlet",
    "body": "router-outlet"
  }
}