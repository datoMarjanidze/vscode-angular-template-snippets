{  
  "NgClass": {
    "prefix": ["ngClass", "[ngClass]"],
    "body": "[ngClass]=\"${0:expression}\""
  },
  "NgComponentOutlet": {
    "prefix": ["ngComponentOutlet", "*ngComponentOutlet"],
    "body": "*ngComponentOutlet=\"${0:componentTypeExpression}\""
  },
  "NgComponentOutletInjector": {
    "prefix": "ngComponentOutletInjector",
    "body": "ngComponentOutletInjector=\"${0:expression}\""
  },
  "NgComponentOutletContent": {
    "prefix": "ngComponentOutletContent",
    "body": "ngComponentOutletContent=\"${0:expression}\""
  },
  "NgComponentOutletNgModuleFactory": {
    "prefix": "ngComponentOutletNgModuleFactory",
    "body": "ngComponentOutletNgModuleFactory=\"${0:expression}\""
  },
  "NgFor": {
    "prefix": ["ngFor", "*ngFor"],
    "body": "*ngFor=\"let ${1:item} of ${0:items}\""
  },
  "NgForWithIndex": {
    "prefix": ["ngForWithIndex", "*ngForWithIndex"],
    "body": "*ngFor=\"let ${1:item} of ${2:items}; index as ${0:i}\""
  },
  "NgForWithCount": {
    "prefix": ["ngForWithCount", "*ngForWithCount"],
    "body": "*ngFor=\"let ${1:item} of ${2:items}; count as ${0:c}\""
  },
  "NgForWithFirst": {
    "prefix": ["ngForWithFirst", "*ngForWithFirst"],
    "body": "*ngFor=\"let ${1:item} of ${2:items}; first as ${0:isFirst}\""
  },
  "NgForWithLast": {
    "prefix": ["ngForWithLast", "*ngForWithLast"],
    "body": "*ngFor=\"let ${1:item} of ${2:items}; last as ${0:isLast}\""
  },
  "NgForWithEven": {
    "prefix": ["ngForWithEven", "*ngForWithEven"],
    "body": "*ngFor=\"let ${1:item} of ${2:items}; even as ${0:isEven}\""
  },
  "NgForWithOdd": {
    "prefix": ["ngForWithOdd", "*ngForWithOdd"],
    "body": "*ngFor=\"let ${1:item} of ${2:items}; odd as ${0:isOdd}\""
  },
  "NgForWithTrackBy": {
    "prefix": ["ngForWithTrackBy", "*ngForWithTrackBy"],
    "body": "*ngFor=\"let ${1:item} of ${2:items}; trackBy: ${0:trackByFn}\""
  },
  "NgForOf": {
    "prefix": ["ngForOf", "[ngForOf]"],
    "body": "[ngForOf]=\"${0:items}\""
  },
  "NgForTrackBy": {
    "prefix": ["ngForTrackBy", "[ngForTrackBy]"],
    "body": "[ngForTrackBy]=\"${0:trackByFn}\""
  },
  "NgIf": {
    "prefix": ["ngIf", "*ngIf"],
    "body": "*ngIf=\"${0:condition}\""
  },
  "NgIfElse": {
    "prefix": ["ngIfElse", "*ngIfElse"],
    "body": "*ngIf=\"${1:condition} else ${0:elseBlock}\""
  },
  "NgIfThenElse": {
    "prefix": ["ngIfThenElse", "*ngIfThenElse"],
    "body": "*ngIf=\"${1:condition}; then ${2:thenBlock} else ${0:elseBlock}\""
  },
  "NgIfAs": {
    "prefix": ["ngIfAs", "*ngIfAs"],
    "body": "*ngIf=\"${1:condition} as ${0:value}\""
  },
  "NgIfAsync": {
    "prefix": ["ngIfAsync", "*ngIfAsync"],
    "body": "*ngIf=\"${0:condition} | async\""
  },
  "NgIfAsyncAs": {
    "prefix": ["ngIfAsyncAs", "*ngIfAsyncAs"],
    "body": "*ngIf=\"${1:condition} | async as ${0:value}\""
  },
  "NgPlural": {
    "prefix": ["ngPlural", "[ngPlural]"],
    "body": "[ngPlural]=\"${0:value}\""
  },
  "NgPluralCase": {
    "prefix": "ngPluralCase",
    "body": "ngPluralCase=\"${0:value}\""
  },
  "NgStyle": {
    "prefix": ["ngStyle", "[ngStyle]"],
    "body": "[ngStyle]=\"{${1:klass}: ${0:expression}}\""
  },
  "NgSwitch": {
    "prefix": ["ngSwitch", "[ngSwitch]"],
    "body": "[ngSwitch]=\"${0:switch_expression}\""
  },
  "NgSwitchCase": {
    "prefix": ["ngSwitchCase", "*ngSwitchCase"],
    "body": "*ngSwitchCase=\"${0:match_expression}\""
  },
  "NgSwitchDefault": {
    "prefix": ["ngSwitchDefault", "*ngSwitchDefault"],
    "body": "*ngSwitchDefault"
  },
  "NgTemplateOutlet": {
    "prefix": ["ngTemplateOutlet", "*ngTemplateOutlet"],
    "body": "*ngTemplateOutlet=\"${0:templateRefExp}\""
  },
  "NgTemplateOutletWithContext": {
    "prefix": ["ngTemplateOutletWithContext", "*ngTemplateOutletWithContext"],
    "body": "*ngTemplateOutlet=\"${1:templateRefExp}; context: ${0:contextExp}\""
  }
}
