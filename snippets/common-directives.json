{  
  "NgClass": {
    "prefix": ["ngClass", "[ngClass]"],
    "body": "[ngClass]=\"${1:expression}\"$0"
  },
  "NgComponentOutlet": {
    "prefix": ["ngComponentOutlet", "*ngComponentOutlet"],
    "body": "*ngComponentOutlet=\"${1:componentTypeExpression}\"$0"
  },
  "NgComponentOutletInjector": {
    "prefix": "ngComponentOutletInjector",
    "body": "ngComponentOutletInjector=\"${1:expression}\"$0"
  },
  "NgComponentOutletContent": {
    "prefix": "ngComponentOutletContent",
    "body": "ngComponentOutletContent=\"${1:expression}\"$0"
  },
  "NgComponentOutletNgModuleFactory": {
    "prefix": "ngComponentOutletNgModuleFactory",
    "body": "ngComponentOutletNgModuleFactory=\"${1:expression}\"$0"
  },
  "NgFor": {
    "prefix": ["ngFor", "*ngFor"],
    "body": "*ngFor=\"let ${1:item} of ${2:items}\"$0"
  },
  "NgForWithIndex": {
    "prefix": ["ngForWithIndex", "*ngForWithIndex"],
    "body": "*ngFor=\"let ${1:item} of ${2:items}; index as ${3:i}\"$0"
  },
  "NgForWithCount": {
    "prefix": ["ngForWithCount", "*ngForWithCount"],
    "body": "*ngFor=\"let ${1:item} of ${2:items}; count as ${3:c}\"$0"
  },
  "NgForWithFirst": {
    "prefix": ["ngForWithFirst", "*ngForWithFirst"],
    "body": "*ngFor=\"let ${1:item} of ${2:items}; first as ${3:isFirst}\"$0"
  },
  "NgForWithLast": {
    "prefix": ["ngForWithLast", "*ngForWithLast"],
    "body": "*ngFor=\"let ${1:item} of ${2:items}; last as ${3:isLast}\"$0"
  },
  "NgForWithEven": {
    "prefix": ["ngForWithEven", "*ngForWithEven"],
    "body": "*ngFor=\"let ${1:item} of ${2:items}; even as ${3:isEven}\"$0"
  },
  "NgForWithOdd": {
    "prefix": ["ngForWithOdd", "*ngForWithOdd"],
    "body": "*ngFor=\"let ${1:item} of ${2:items}; odd as ${3:isOdd}\"$0"
  },
  "NgForWithTrackBy": {
    "prefix": ["ngForWithTrackBy", "*ngForWithTrackBy"],
    "body": "*ngFor=\"let ${1:item} of ${2:items}; trackBy: ${3:trackByFn}\"$0"
  },
  "NgForOf": {
    "prefix": ["ngForOf", "[ngForOf]"],
    "body": "[ngForOf]=\"${1:items}\"$0"
  },
  "NgForTrackBy": {
    "prefix": ["ngForTrackBy", "[ngForTrackBy]"],
    "body": "[ngForTrackBy]=\"${1:trackByFn}\"$0"
  },
  "NgIf": {
    "prefix": ["ngIf", "*ngIf"],
    "body": "*ngIf=\"${1:condition}\"$0"
  },
  "NgIfElse": {
    "prefix": ["ngIfElse", "*ngIfElse"],
    "body": "*ngIf=\"${1:condition} else ${2:elseBlock}\"$0"
  },
  "NgIfThenElse": {
    "prefix": ["ngIfThenElse", "*ngIfThenElse"],
    "body": "*ngIf=\"${1:condition}; then ${2:thenBlock} else ${3:elseBlock}\"$0"
  },
  "NgIfAs": {
    "prefix": ["ngIfAs", "*ngIfAs"],
    "body": "*ngIf=\"${1:condition} as ${2:value}\"$0"
  },
  "NgIfAsync": {
    "prefix": ["ngIfAsync", "*ngIfAsync"],
    "body": "*ngIf=\"${1:condition} | async\"$0"
  },
  "NgIfAsyncAs": {
    "prefix": ["ngIfAsyncAs", "*ngIfAsyncAs"],
    "body": "*ngIf=\"${1:condition} | async as ${2:value}\"$0"
  },
  "NgPlural": {
    "prefix": ["ngPlural", "[ngPlural]"],
    "body": "[ngPlural]=\"${1:value}\"$0"
  },
  "NgPluralCase": {
    "prefix": "ngPluralCase",
    "body": "ngPluralCase=\"${1:value}\"$0"
  },
  "NgStyle": {
    "prefix": ["ngStyle", "[ngStyle]"],
    "body": "[ngStyle]=\"{${1:klass}: ${2:expression}}\"$0"
  },
  "NgSwitch": {
    "prefix": ["ngSwitch", "[ngSwitch]"],
    "body": "[ngSwitch]=\"${1:switch_expression}\"$0"
  },
  "NgSwitchCase": {
    "prefix": ["ngSwitchCase", "*ngSwitchCase"],
    "body": "*ngSwitchCase=\"${1:match_expression}\"$0"
  },
  "NgSwitchDefault": {
    "prefix": ["ngSwitchDefault", "*ngSwitchDefault"],
    "body": "*ngSwitchDefault"
  },
  "NgTemplateOutlet": {
    "prefix": ["ngTemplateOutlet", "*ngTemplateOutlet"],
    "body": "*ngTemplateOutlet=\"${1:templateRefExp}\"$0"
  },
  "NgTemplateOutletWithContext": {
    "prefix": ["ngTemplateOutletWithContext", "*ngTemplateOutletWithContext"],
    "body": "*ngTemplateOutlet=\"${1:templateRefExp}; context: ${2:contextExp}\"$0"
  }
}
